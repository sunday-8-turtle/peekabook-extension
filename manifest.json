{
  "name": "Pickabook",
  "description": "Pickabook dev version.",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "./background/index.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.com/*"
      ],
      "js": [
        "contentScript/index.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {}
  },
  "icons": {}
}
